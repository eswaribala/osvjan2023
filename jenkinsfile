#!groovy
pipeline{
 agent any
 
  tools {
    maven 'maven'
  }
 
 stages{
 
   stage("build"){
   
       steps{
       
          bat "mvn -f ./customerapi clean install -DskipTests"
          bat "cd customerapi  & docker-compose up -d"
                                   
       }
   
   }
 
 }
 
 
 
}
